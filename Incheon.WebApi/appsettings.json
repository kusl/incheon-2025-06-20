{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Analytics": {
    "EnableAnalytics": true,
    "BatchSize": 100,
    "CacheExpiryMinutes": 5,
    "MaxEventsPerUser": 10000,
    "RetentionDays": 90,
    "ExcludedPaths": ["/health", "/metrics", "/favicon.ico"]
  }
}
